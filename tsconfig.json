{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["DOM", "ES2015"],
    "jsx": "react",
    "moduleResolution": "node",
    "types": [],
    "allowJs": false,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true
  },
  "exclude": ["node_modules"]
}
